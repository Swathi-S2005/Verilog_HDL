#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57a3114a6bf0 .scope module, "tb_shiftreg" "tb_shiftreg" 2 1;
 .timescale 0 0;
v0x57a3114cfb60_0 .var "clk", 0 0;
v0x57a3114cfc20_0 .var "in", 0 0;
v0x57a3114cfcf0_0 .net "out", 0 0, v0x57a3114cf820_0;  1 drivers
v0x57a3114cfdf0_0 .var "rst", 0 0;
S_0x57a3114ba660 .scope module, "uut" "shiftreg" 2 4, 3 1 0, S_0x57a3114a6bf0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "in";
    .port_info 3 /OUTPUT 1 "out";
v0x57a3114ba8d0_0 .net "clk", 0 0, v0x57a3114cfb60_0;  1 drivers
v0x57a3114cf760_0 .net "in", 0 0, v0x57a3114cfc20_0;  1 drivers
v0x57a3114cf820_0 .var "out", 0 0;
v0x57a3114cf8f0_0 .net "rst", 0 0, v0x57a3114cfdf0_0;  1 drivers
v0x57a3114cf9b0_0 .var "shift", 3 0;
E_0x57a311480dc0 .event posedge, v0x57a3114cf8f0_0, v0x57a3114ba8d0_0;
    .scope S_0x57a3114ba660;
T_0 ;
    %wait E_0x57a311480dc0;
    %load/vec4 v0x57a3114cf8f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x57a3114cf9b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x57a3114cf760_0;
    %load/vec4 v0x57a3114cf9b0_0;
    %parti/s 3, 0, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x57a3114cf9b0_0, 0;
    %load/vec4 v0x57a3114cf9b0_0;
    %parti/s 1, 3, 3;
    %assign/vec4 v0x57a3114cf820_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x57a3114a6bf0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x57a3114cfb60_0;
    %inv;
    %store/vec4 v0x57a3114cfb60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x57a3114a6bf0;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "shiftreg.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57a3114a6bf0 {0 0 0};
    %vpi_call 2 13 "$monitor", "Time=%0t,in=%b,rst=%b,out=%b", $time, v0x57a3114cfc20_0, v0x57a3114cfdf0_0, v0x57a3114cfcf0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57a3114cfb60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfdf0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfb60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57a3114cfdf0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57a3114cfc20_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
