:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61172a9ae1a0 .scope module, "tb_shiftreg" "tb_shiftreg" 2 1;
 .timescale 0 0;
v0x61172a9d7b20_0 .var "clk", 0 0;
v0x61172a9d7be0_0 .var "in", 3 0;
v0x61172a9d7cb0_0 .var "load", 0 0;
v0x61172a9d7db0_0 .net "out", 0 0, v0x61172a9d77d0_0;  1 drivers
v0x61172a9d7e80_0 .var "rst", 0 0;
S_0x61172a9c1e00 .scope module, "uut" "piso" 2 6, 3 1 0, S_0x61172a9ae1a0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /INPUT 1 "load";
    .port_info 4 /OUTPUT 1 "out";
v0x61172a9c2000_0 .net "clk", 0 0, v0x61172a9d7b20_0;  1 drivers
v0x61172a9d7620_0 .net "in", 3 0, v0x61172a9d7be0_0;  1 drivers
v0x61172a9d7700_0 .net "load", 0 0, v0x61172a9d7cb0_0;  1 drivers
v0x61172a9d77d0_0 .var "out", 0 0;
v0x61172a9d7890_0 .net "rst", 0 0, v0x61172a9d7e80_0;  1 drivers
v0x61172a9d79a0_0 .var "shift", 3 0;
E_0x61172a9c0320 .event posedge, v0x61172a9d7890_0, v0x61172a9c2000_0;
    .scope S_0x61172a9c1e00;
T_0 ;
    %wait E_0x61172a9c0320;
    %load/vec4 v0x61172a9d7890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x61172a9d79a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x61172a9d7700_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x61172a9d7620_0;
    %assign/vec4 v0x61172a9d79a0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x61172a9d79a0_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x61172a9d7620_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 4;
    %assign/vec4 v0x61172a9d79a0_0, 0;
    %load/vec4 v0x61172a9d79a0_0;
    %parti/s 1, 3, 3;
    %assign/vec4 v0x61172a9d77d0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61172a9ae1a0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x61172a9d7b20_0;
    %inv;
    %store/vec4 v0x61172a9d7b20_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x61172a9ae1a0;
T_2 ;
    %vpi_call 2 14 "$dumpfile", "shiftreg.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x61172a9ae1a0 {0 0 0};
    %vpi_call 2 16 "$monitor", "Time=%0t,in=%b,rst=%b,load=%b|out=%b", $time, v0x61172a9d7be0_0, v0x61172a9d7e80_0, v0x61172a9d7cb0_0, v0x61172a9d7db0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61172a9d7b20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61172a9d7e80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61172a9d7cb0_0, 0, 1;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61172a9d7cb0_0, 0, 1;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61172a9d7e80_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61172a9d7cb0_0, 0, 1;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61172a9d7cb0_0, 0, 1;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x61172a9d7be0_0, 0, 4;
    %delay 10, 0;
    %delay 200, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_2;
