S_0x647b731befa0 .scope module, "tb_shiftreg" "tb_shiftreg" 2 1;
 .timescale 0 0;
v0x647b731e8620_0 .var "clk", 0 0;
v0x647b731e86e0_0 .var "in", 3 0;
v0x647b731e87b0_0 .net "out", 0 0, v0x647b731e82d0_0;  1 drivers
v0x647b731e88b0_0 .var "rst", 0 0;
S_0x647b731d2e00 .scope module, "uut" "piso" 2 5, 3 1 0, S_0x647b731befa0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /INPUT 1 "load";
    .port_info 4 /OUTPUT 1 "out";
v0x647b731d3000_0 .net "clk", 0 0, v0x647b731e8620_0;  1 drivers
v0x647b731e8120_0 .net "in", 3 0, v0x647b731e86e0_0;  1 drivers
o0x7572da3a0078 .functor BUFZ 1, C4<z>; HiZ drive
v0x647b731e8200_0 .net "load", 0 0, o0x7572da3a0078;  0 drivers
v0x647b731e82d0_0 .var "out", 0 0;
v0x647b731e8390_0 .net "rst", 0 0, v0x647b731e88b0_0;  1 drivers
v0x647b731e84a0_0 .var "shift", 3 0;
E_0x647b731d1320 .event posedge, v0x647b731e8390_0, v0x647b731d3000_0;
    .scope S_0x647b731d2e00;
T_0 ;
    %wait E_0x647b731d1320;
    %load/vec4 v0x647b731e8390_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x647b731e84a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x647b731e8200_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x647b731e8120_0;
    %assign/vec4 v0x647b731e84a0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x647b731e84a0_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x647b731e8120_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 4;
    %assign/vec4 v0x647b731e84a0_0, 0;
    %load/vec4 v0x647b731e84a0_0;
    %parti/s 1, 3, 3;
    %assign/vec4 v0x647b731e82d0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x647b731befa0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x647b731e8620_0;
    %inv;
    %store/vec4 v0x647b731e8620_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x647b731befa0;
T_2 ;
    %vpi_call 2 12 "$dumpfile", "shiftreg.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x647b731befa0 {0 0 0};
    %vpi_call 2 14 "$monitor", "Time=%0t,in=%b,rst=%b,out=%b", $time, v0x647b731e86e0_0, v0x647b731e88b0_0, v0x647b731e87b0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647b731e8620_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x647b731e88b0_0, 0, 1;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647b731e88b0_0, 0, 1;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x647b731e86e0_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
