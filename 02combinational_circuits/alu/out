    %load/vec4 v0x5c633aea3240_0;
    %mod;
    %assign/vec4 v0x5c633aea33f0_0, 0;
    %jmp T_0.9;
T_0.4 ;
    %load/vec4 v0x5c633ae8e090_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %assign/vec4 v0x5c633aea33f0_0, 0;
