#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ac4fe6952a0 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale 0 0;
v0x5ac4fe6a8d40_0 .var "clk", 0 0;
v0x5ac4fe6a8e00_0 .var "ori", 0 0;
v0x5ac4fe6a8ed0_0 .net "q", 3 0, v0x5ac4fe6a8ae0_0;  1 drivers
v0x5ac4fe6a8fd0_0 .var "rst", 0 0;
S_0x5ac4fe695430 .scope module, "uut" "counter" 2 4, 3 1 0, S_0x5ac4fe6952a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "ori";
    .port_info 3 /OUTPUT 4 "q";
v0x5ac4fe65b490_0 .net "clk", 0 0, v0x5ac4fe6a8d40_0;  1 drivers
v0x5ac4fe6a8a20_0 .net "ori", 0 0, v0x5ac4fe6a8e00_0;  1 drivers
v0x5ac4fe6a8ae0_0 .var "q", 3 0;
v0x5ac4fe6a8bd0_0 .net "rst", 0 0, v0x5ac4fe6a8fd0_0;  1 drivers
E_0x5ac4fe65c6a0 .event posedge, v0x5ac4fe6a8bd0_0, v0x5ac4fe65b490_0;
    .scope S_0x5ac4fe695430;
T_0 ;
    %wait E_0x5ac4fe65c6a0;
    %load/vec4 v0x5ac4fe6a8bd0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5ac4fe6a8ae0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ac4fe6a8a20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x5ac4fe6a8ae0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5ac4fe6a8ae0_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5ac4fe6a8ae0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5ac4fe6a8ae0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5ac4fe6952a0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5ac4fe6a8d40_0;
    %inv;
    %store/vec4 v0x5ac4fe6a8d40_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ac4fe6952a0;
T_2 ;
    %vpi_call 2 11 "$monitor", "Time=%1t|clk=%b|rst=%b|ori=%b|q=%b", $time, v0x5ac4fe6a8d40_0, v0x5ac4fe6a8fd0_0, v0x5ac4fe6a8e00_0, v0x5ac4fe6a8ed0_0 {0 0 0};
    %vpi_call 2 12 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5ac4fe6952a0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac4fe6a8d40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ac4fe6a8fd0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ac4fe6a8e00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac4fe6a8fd0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ac4fe6a8e00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac4fe6a8e00_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
