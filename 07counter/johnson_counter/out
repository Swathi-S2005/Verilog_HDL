#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c13a8d78880 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale 0 0;
v0x5c13a8d8bd50_0 .var "clk", 0 0;
v0x5c13a8d8be20_0 .net "q", 3 0, v0x5c13a8d8bb50_0;  1 drivers
v0x5c13a8d8bef0_0 .var "rst", 0 0;
S_0x5c13a8d78a10 .scope module, "uut" "counter" 2 4, 3 1 0, S_0x5c13a8d78880;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x5c13a8d78c60_0 .net "clk", 0 0, v0x5c13a8d8bd50_0;  1 drivers
v0x5c13a8d8bb50_0 .var "q", 3 0;
v0x5c13a8d8bc30_0 .net "rst", 0 0, v0x5c13a8d8bef0_0;  1 drivers
E_0x5c13a8d3eb20 .event posedge, v0x5c13a8d8bc30_0, v0x5c13a8d78c60_0;
    .scope S_0x5c13a8d78a10;
T_0 ;
    %wait E_0x5c13a8d3eb20;
    %load/vec4 v0x5c13a8d8bc30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5c13a8d8bb50_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c13a8d8bb50_0;
    %parti/s 1, 3, 3;
    %inv;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5c13a8d8bb50_0, 4, 5;
    %load/vec4 v0x5c13a8d8bb50_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5c13a8d8bb50_0, 4, 5;
    %load/vec4 v0x5c13a8d8bb50_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5c13a8d8bb50_0, 4, 5;
    %load/vec4 v0x5c13a8d8bb50_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5c13a8d8bb50_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c13a8d78880;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5c13a8d8bd50_0;
    %inv;
    %store/vec4 v0x5c13a8d8bd50_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5c13a8d78880;
T_2 ;
    %vpi_call 2 10 "$monitor", "Time=%0t|clk=%b|rst=%b|q=%b", $time, v0x5c13a8d8bd50_0, v0x5c13a8d8bef0_0, v0x5c13a8d8be20_0 {0 0 0};
    %vpi_call 2 11 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c13a8d78880 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c13a8d8bd50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c13a8d8bef0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c13a8d8bef0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_johnson.v";
    "johnson_counter.v";
