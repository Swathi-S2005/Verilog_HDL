:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55974f318700 .scope module, "tb_square" "tb_square" 2 1;
 .timescale 0 0;
v0x55974f32b960_0 .var "a", 7 0;
v0x55974f32ba50_0 .var "b", 7 0;
v0x55974f32bb20_0 .net "out", 16 0, L_0x55974f32bc20;  1 drivers
S_0x55974f318890 .scope module, "uut" "square_no" 2 4, 3 1 0, S_0x55974f318700;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "a";
    .port_info 1 /INPUT 8 "b";
    .port_info 2 /OUTPUT 17 "out";
v0x55974f32b610_0 .net "a", 7 0, v0x55974f32b960_0;  1 drivers
v0x55974f32b710_0 .net "b", 7 0, v0x55974f32ba50_0;  1 drivers
v0x55974f32b7f0_0 .net "out", 16 0, L_0x55974f32bc20;  alias, 1 drivers
L_0x55974f32bc20 .ufunc/vec4 TD_tb_square.uut.square, 17, v0x55974f32b960_0, v0x55974f32ba50_0 (v0x55974f32b470_0, v0x55974f32b550_0) S_0x55974f2de490;
S_0x55974f2de490 .scope function.vec4.s17, "square" "square" 3 4, 3 4 0, S_0x55974f318890;
 .timescale 0 0;
; Variable square is vec4 return value of scope S_0x55974f2de490
v0x55974f32b470_0 .var "x", 7 0;
v0x55974f32b550_0 .var "y", 7 0;
TD_tb_square.uut.square ;
    %load/vec4 v0x55974f32b470_0;
    %pad/u 17;
    %load/vec4 v0x55974f32b550_0;
    %pow;
    %ret/vec4 0, 0, 17;  Assign to square (store_vec4_to_lval)
    %end;
    .scope S_0x55974f318700;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "square.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55974f318700 {0 0 0};
    %vpi_call 2 11 "$monitor", "Time=%0t|a=%d|b=%d|out=%d", $time, v0x55974f32b960_0, v0x55974f32ba50_0, v0x55974f32bb20_0 {0 0 0};
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x55974f32b960_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55974f32ba50_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x55974f32b960_0, 0, 8;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x55974f32ba50_0, 0, 8;
    %delay 10, 0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
