#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x638a1ad05ff0 .scope module, "tb_gray" "tb_gray" 2 1;
 .timescale 0 0;
v0x638a1ad1a250_0 .var "b", 3 0;
v0x638a1ad1a310_0 .net "g", 3 0, L_0x638a1ad1a4b0;  1 drivers
v0x638a1ad1a3e0_0 .var/i "i", 31 0;
S_0x638a1ad06180 .scope module, "uut" "graycode" 2 4, 3 1 0, S_0x638a1ad05ff0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "b";
    .port_info 1 /OUTPUT 4 "g";
v0x638a1ad1a010_0 .net "b", 3 0, v0x638a1ad1a250_0;  1 drivers
v0x638a1ad1a110_0 .net "g", 3 0, L_0x638a1ad1a4b0;  alias, 1 drivers
L_0x638a1ad1a4b0 .ufunc/vec4 TD_tb_gray.uut.gray, 4, v0x638a1ad1a250_0 (v0x638a1ad19f30_0) S_0x638a1accb490;
S_0x638a1accb490 .scope function.vec4.s4, "gray" "gray" 3 4, 3 4 0, S_0x638a1ad06180;
 .timescale 0 0;
; Variable gray is vec4 return value of scope S_0x638a1accb490
v0x638a1ad19f30_0 .var "in", 3 0;
TD_tb_gray.uut.gray ;
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray (store_vec4_to_lval)
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray (store_vec4_to_lval)
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray (store_vec4_to_lval)
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x638a1ad19f30_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to gray (store_vec4_to_lval)
    %end;
    .scope S_0x638a1ad05ff0;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "code.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x638a1ad05ff0 {0 0 0};
    %vpi_call 2 11 "$monitor", "Time=%0t|b=%b|g=%b", $time, v0x638a1ad1a250_0, v0x638a1ad1a310_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x638a1ad1a3e0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x638a1ad1a3e0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x638a1ad1a3e0_0;
    %pad/s 4;
    %store/vec4 v0x638a1ad1a250_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x638a1ad1a3e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x638a1ad1a3e0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %delay 100, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_gray.v";
    "graycode.v";
