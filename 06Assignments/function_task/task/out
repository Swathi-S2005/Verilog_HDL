:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x625b9a13a530 .scope module, "tb_sum" "tb_sum" 2 1;
 .timescale 0 0;
v0x625b9a186cc0_0 .var "a", 3 0;
v0x625b9a186dc0_0 .var "b", 3 0;
v0x625b9a186ea0_0 .var "out", 4 0;
S_0x625b9a13a6c0 .scope task, "sum" "sum" 2 4, 2 4 0, S_0x625b9a13a530;
 .timescale 0 0;
v0x625b9a162a90_0 .var "sum", 4 0;
v0x625b9a186b20_0 .var "x", 3 0;
v0x625b9a186c00_0 .var "y", 3 0;
TD_tb_sum.sum ;
    %load/vec4 v0x625b9a186b20_0;
    %pad/u 5;
    %load/vec4 v0x625b9a186c00_0;
    %pad/u 5;
    %add;
    %store/vec4 v0x625b9a162a90_0, 0, 5;
    %end;
    .scope S_0x625b9a13a530;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "sum.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x625b9a13a530 {0 0 0};
    %vpi_call 2 15 "$monitor", "Time=%0t|a=%b|b=%b|out=%b|", $time, v0x625b9a186cc0_0, v0x625b9a186dc0_0, v0x625b9a186ea0_0 {0 0 0};
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x625b9a186cc0_0, 0, 4;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x625b9a186dc0_0, 0, 4;
    %load/vec4 v0x625b9a186cc0_0;
    %store/vec4 v0x625b9a186b20_0, 0, 4;
    %load/vec4 v0x625b9a186dc0_0;
    %store/vec4 v0x625b9a186c00_0, 0, 4;
    %fork TD_tb_sum.sum, S_0x625b9a13a6c0;
    %join;
    %load/vec4 v0x625b9a162a90_0;
    %store/vec4 v0x625b9a186ea0_0, 0, 5;
    %delay 10, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x625b9a186cc0_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x625b9a186dc0_0, 0, 4;
    %load/vec4 v0x625b9a186cc0_0;
    %store/vec4 v0x625b9a186b20_0, 0, 4;
    %load/vec4 v0x625b9a186dc0_0;
    %store/vec4 v0x625b9a186c00_0, 0, 4;
    %fork TD_tb_sum.sum, S_0x625b9a13a6c0;
    %join;
    %load/vec4 v0x625b9a162a90_0;
    %store/vec4 v0x625b9a186ea0_0, 0, 5;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x625b9a186cc0_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x625b9a186dc0_0, 0, 4;
    %load/vec4 v0x625b9a186cc0_0;
    %store/vec4 v0x625b9a186b20_0, 0, 4;
    %load/vec4 v0x625b9a186dc0_0;
    %store/vec4 v0x625b9a186c00_0, 0, 4;
    %fork TD_tb_sum.sum, S_0x625b9a13a6c0;
    %join;
    %load/vec4 v0x625b9a162a90_0;
    %store/vec4 v0x625b9a186ea0_0, 0, 5;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
