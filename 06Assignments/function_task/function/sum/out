v0x5c88b943dcc0_0 .var "y", 3 0;
TD_tb_sum.uut.sum ;
    %load/vec4 v0x5c88b943dd80_0;
    %pad/u 5;
    %load/vec4 v0x5c88b943de80_0;
    %pad/u 5;
    %add;
    %ret/vec4 0, 0, 5;  Assign to sum (store_vec4_to_lval)
    %end;
    .scope S_0x5c88b942a6d0;
T_1 ;
