    %store/vec4 v0x5c88b943e0d0_0, 0, 4;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x5c88b943e1c0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5c88b943e0d0_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5c88b943e1c0_0, 0, 4;
    %delay 10, 0;
