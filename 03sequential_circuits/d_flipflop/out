    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d3f65e5e3d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d3f65e5e690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d3f65e5e4e0_0, 0, 1;
