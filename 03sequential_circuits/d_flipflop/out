v0x5d3f65e5e2b0_0 .net "t", 0 0, L_0x5d3f65e5e780;  1 drivers
S_0x5d3f65e0c6e0 .scope module, "d1" "t_ff" 3 19, 3 1 0, S_0x5d3f65e0c490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "t";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /OUTPUT 1 "q";
v0x5d3f65e47640_0 .net "clk", 0 0, v0x5d3f65e5e3d0_0;  alias, 1 drivers
v0x5d3f65e5dcb0_0 .var "q", 0 0;
v0x5d3f65e5dd70_0 .net "rst", 0 0, v0x5d3f65e5e690_0;  alias, 1 drivers
v0x5d3f65e5de10_0 .net "t", 0 0, L_0x5d3f65e5e780;  alias, 1 drivers
E_0x5d3f65e46480 .event posedge, v0x5d3f65e5dd70_0, v0x5d3f65e47640_0;
    .scope S_0x5d3f65e0c6e0;
T_0 ;
    %wait E_0x5d3f65e46480;
    %load/vec4 v0x5d3f65e5dd70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d3f65e5dcb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5d3f65e5de10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5d3f65e5dcb0_0;
    %inv;
    %assign/vec4 v0x5d3f65e5dcb0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5d3f65e5dcb0_0;
    %assign/vec4 v0x5d3f65e5dcb0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5d3f65e33a10;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5d3f65e5e3d0_0;
    %inv;
    %store/vec4 v0x5d3f65e5e3d0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5d3f65e33a10;
T_2 ;
