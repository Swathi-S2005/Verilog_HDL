    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /OUTPUT 1 "q";
L_0x5d3f65e5e780 .functor XOR 1, v0x5d3f65e5e4e0_0, v0x5d3f65e5dcb0_0, C4<0>, C4<0>;
v0x5d3f65e5df80_0 .net "clk", 0 0, v0x5d3f65e5e3d0_0;  1 drivers
v0x5d3f65e5e040_0 .net "d", 0 0, v0x5d3f65e5e4e0_0;  1 drivers
v0x5d3f65e5e0e0_0 .net "q", 0 0, v0x5d3f65e5dcb0_0;  alias, 1 drivers
