    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x602ca3313e00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x602ca3313c70_0, 0, 1;
    %delay 10, 0;
